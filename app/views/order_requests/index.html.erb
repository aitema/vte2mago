<p id="notice"><%= notice %></p>

<h1>Listing Order Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_requests.each do |order_request| %>
      <tr>
        <td><%= link_to 'Show', order_request %></td>
        <td><%= link_to 'Edit', edit_order_request_path(order_request) %></td>
        <td><%= link_to 'Destroy', order_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order request', new_order_request_path %>
