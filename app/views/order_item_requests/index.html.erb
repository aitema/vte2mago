<p id="notice"><%= notice %></p>

<h1>Listing Order Item Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_item_requests.each do |order_item_request| %>
      <tr>
        <td><%= link_to 'Show', order_item_request %></td>
        <td><%= link_to 'Edit', edit_order_item_request_path(order_item_request) %></td>
        <td><%= link_to 'Destroy', order_item_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order item request', new_order_item_request_path %>
